<?xml version="1.0" ?>
<robot name="simple_humanoid" xmlns:xacro="http://www.ros.org/wiki/xacro">
  <material name="white">
    <color rgba="1.0 1.0 1.0 1.0"/>
  </material>
  <!-- import empty link -->
  <link name="base_link">
  </link>
  <!-- base joints -->
  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="body_link"/>
    <origin rpy="0 0 0" xyz="0 0 0.4"/>
  </joint>
  <!-- base body -->
  <link name="body_link">
    <inertial>
      <mass value="2.0"/>
      <origin xyz="0 0 0"/>
      <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.15"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
        <box size="0.1 0.1 0.15"/>
      </geometry>
    </collision>
  </link>
  <!-- waist joints -->
  <joint name="waist_roll_joint" type="revolute">
    <parent link="body_link"/>
    <child link="waist_link"/>
    <origin rpy="0 0 0" xyz="0 0 -0.1"/>
    <limit effort="1000" lower="-2.35619449019" upper="2.35619449019" velocity="6.0"/>
    <axis xyz="1 0 0"/>
  </joint>
  <!-- waist link -->
  <link name="waist_link">
    <inertial>
      <mass value="0.2"/>
      <origin xyz="0.0 0 -0.03"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.03"/>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.03"/>
      <geometry>
        <box size="0.1 0.1 0.05"/>
      </geometry>
    </collision>
  </link>
  <!-- left waist pitch joints -->
  <joint name="left_waist_pitch_joint" type="revolute">
    <parent link="waist_link"/>
    <child link="left_thigh_pitch_link"/>
    <origin rpy="0 0 0" xyz="0 0.05 -0.03"/>
    <limit effort="1000" lower="-2.35619449019" upper="2.35619449019" velocity="1.0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <!-- left_thigh_pitch_link -->
  <link name="left_thigh_pitch_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 -0.13"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.13"/>
      <geometry>
        <cylinder length="0.26" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.13"/>
      <geometry>
        <cylinder length="0.26" radius="0.01"/>
      </geometry>
    </collision>
  </link>
  <!-- right waist pitch joints -->
  <joint name="right_waist_pitch_joint" type="revolute">
    <parent link="waist_link"/>
    <child link="right_thigh_pitch_link"/>
    <origin rpy="0 0 0" xyz="0 -0.05 -0.03"/>
    <limit effort="1000" lower="-2.35619449019" upper="2.35619449019" velocity="1.0"/>
    <axis xyz="0 1 0"/>
  </joint>
  <!-- right thigh pitch link -->
  <link name="right_thigh_pitch_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 -0.13"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 -0.13"/>
      <geometry>
        <cylinder length="0.26" radius="0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 -0.13"/>
      <geometry>
        <cylinder length="0.26" radius="0.01"/>
      </geometry>
    </collision>
  </link>
  <!-- left ankle yaw joints -->
  <joint name="left_ankle_yaw_joint" type="revolute">
    <parent link="left_thigh_pitch_link"/>
    <child link="left_foot_link"/>
    <origin rpy="0 0 0" xyz="0 0 -0.26"/>
    <limit effort="1000" lower="-2.35619449019" upper="2.35619449019" velocity="1.0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <!-- left foot link -->
  <link name="left_foot_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0.005"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <box size="0.15 0.05 0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <box size="0.15 0.05 0.01"/>
      </geometry>
    </collision>
  </link>
  <!-- right ankle yaw joints -->
  <joint name="right_ankle_yaw_joint" type="revolute">
    <parent link="right_thigh_pitch_link"/>
    <child link="right_foot_link"/>
    <origin rpy="0 0 0" xyz="0 0 -0.26"/>
    <limit effort="1000" lower="-2.35619449019" upper="2.35619449019" velocity="1.0"/>
    <axis xyz="0 0 1"/>
  </joint>
  <!-- left foot link -->
  <link name="right_foot_link">
    <inertial>
      <mass value="0.1"/>
      <origin xyz="0 0 0.005"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <box size="0.15 0.05 0.01"/>
      </geometry>
      <material name="white"/>
    </visual>
    <collision>
      <origin rpy="0 0 0" xyz="0 0 0.005"/>
      <geometry>
        <box size="0.15 0.05 0.01"/>
      </geometry>
    </collision>
  </link>
</robot>
